{
  "name": "@ap3k/nordpool-chart",
  "version": "0.0.7",
  "exports": "./src/main.ts",
  "compilerOptions": {
    "lib": [
      "deno.window",
      "DOM",
      "DOM.Iterable"
    ]
  },
  "fmt": {
    "indentWidth": 2,
    "singleQuote": true,
    "semiColons": false
  },
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "serve": "deno run --allow-net --allow-read main.ts"
  },
  "imports": {
    "chart.js": "npm:chart.js@^4.5.0",
    "vite": "npm:vite@^5.2.9"
  }
}